{
  "name": "finwa",
  "description": "FinWa visualizes monthly expenses for easier management of personal finances.",
  "version": "2.10.34",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate --fail-on-error",
    "lint": "yarn lint:prettier && yarn lint:es && yarn lint:style",
    "lint:prettier": "prettier --check \"./**/*.{js,json,html,scss,ts,vue,yml}\"",
    "lint:es": "eslint \"./**/*.{js,ts,vue}\"",
    "lint:style": "stylelint \"./**/*.{css,ts,vue}\" --ignore-path .gitignore",
    "test": "jest",
    "pretty": "prettier --write \"./**/*.{js,json,html,scss,ts,vue,yml}\""
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/composition-api": "0.30.0",
    "@nuxtjs/i18n": "7.2.0",
    "@nuxtjs/pwa": "3.3.5",
    "@nuxtjs/sitemap": "2.4.0",
    "@types/uuid": "8.3.1",
    "chart.js": "3.6.0",
    "core-js": "3.19.1",
    "nuxt": "2.15.8",
    "secure-ls": "1.2.6",
    "uuid": "8.3.2",
    "vue": "2.6.14",
    "vue-chart-3": "0.5.11",
    "vue-persistent-storage-manager": "2.0.10",
    "vuex": "3.6.2",
    "vuex-persistedstate": "4.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "14.1.0",
    "@commitlint/config-conventional": "14.1.0",
    "@nuxt/types": "2.15.8",
    "@nuxt/typescript-build": "2.1.0",
    "@nuxtjs/color-mode": "2.1.1",
    "@nuxtjs/eslint-config-typescript": "7.0.2",
    "@nuxtjs/eslint-module": "3.0.2",
    "@nuxtjs/stylelint-module": "4.0.0",
    "@nuxtjs/vuetify": "1.12.1",
    "@typescript-eslint/parser": "5.3.1",
    "@vue/test-utils": "1.2.2",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.3.1",
    "eslint": "8.2.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.5.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-nuxt": "3.0.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-vue": "8.0.3",
    "eslint-plugin-vuetify": "1.1.0",
    "husky": "4.3.8",
    "jest": "27.3.1",
    "jest-canvas-mock": "2.3.1",
    "lint-staged": "12.0.1",
    "postcss-html": "1.2.0",
    "prettier": "2.4.1",
    "stylelint": "14.0.1",
    "stylelint-config-html": "1.0.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-standard": "23.0.0",
    "stylelint-scss": "4.0.0",
    "ts-jest": "27.0.7",
    "vue-jest": "3.0.7",
    "vuetify": "2.5.14"
  }
}
