{
  "name": "finwa",
  "description": "FinWa visualizes monthly expenses for easier management of personal finances.",
  "version": "2.10.157",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate --fail-on-error",
    "lint": "yarn lint:prettier && yarn lint:es && yarn lint:style",
    "lint:prettier": "prettier --check \"./**/*.{js,json,html,scss,ts,vue,yml}\"",
    "lint:es": "eslint \"./**/*.{js,ts,vue}\"",
    "lint:style": "stylelint \"./**/*.{css,ts,vue}\" --ignore-path .gitignore",
    "test": "jest",
    "pretty": "prettier --write \"./**/*.{js,json,html,scss,ts,vue,yml}\""
  },
  "lint-staged": {
    "*.{js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxtjs/composition-api": "0.32.0",
    "@nuxtjs/i18n": "7.3.1",
    "@nuxtjs/pwa": "3.3.5",
    "@nuxtjs/sitemap": "2.4.0",
    "@types/uuid": "9.0.0",
    "chart.js": "3.9.1",
    "core-js": "3.27.2",
    "nuxt": "2.15.8",
    "secure-ls": "1.2.6",
    "uuid": "9.0.0",
    "vue": "2.6.14",
    "vue-chart-3": "2.0.6",
    "vue-persistent-storage-manager": "2.1.1",
    "vuex": "3.6.2",
    "vuex-persistedstate": "4.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.4",
    "@commitlint/config-conventional": "17.4.4",
    "@nuxt/types": "2.16.0",
    "@nuxt/typescript-build": "2.1.0",
    "@nuxtjs/color-mode": "2.1.1",
    "@nuxtjs/eslint-config-typescript": "12.0.0",
    "@nuxtjs/eslint-module": "3.1.0",
    "@nuxtjs/stylelint-module": "4.1.0",
    "@nuxtjs/vuetify": "1.12.3",
    "@typescript-eslint/parser": "5.48.2",
    "@vue/test-utils": "1.3.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "27.5.1",
    "eslint": "8.32.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-nuxt": "4.0.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-vue": "9.9.0",
    "eslint-plugin-vuetify": "1.1.0",
    "husky": "8.0.3",
    "jest": "27.5.1",
    "jest-canvas-mock": "2.4.0",
    "lint-staged": "13.1.0",
    "postcss-html": "1.5.0",
    "prettier": "2.8.3",
    "stylelint": "15.0.0",
    "stylelint-config-html": "1.1.0",
    "stylelint-config-prettier": "9.0.4",
    "stylelint-config-standard": "25.0.0",
    "stylelint-scss": "4.3.0",
    "ts-jest": "27.1.5",
    "vue-jest": "3.0.7",
    "vuetify": "2.6.14"
  }
